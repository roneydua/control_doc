@MastersThesis{Carvalho2013,
  author   = {de Carvalho, Matheus Minelli},
  school   = {Instituto Tecnol{\'{o}}gico de Aeron{\'{a}}utica},
  title    = {{Aceler{\^o}metro angular opto-mec{\^a}nico baseado em grades de Bragg a fibra {\'o}ptica}},
  year     = {2013},
  type     = {mathesis},
  abstract = {Esta disserta{\c{c}}{\~{a}}o apresenta o desenvolvimento inicial de um aceler{\^{o}}metro angular de um eixo utilizando-se grades de Bragg em fibras {\'{o}}pticas como elementos sensores. Para tanto, s{\~{a}}o apresentados os seguintes t{\'{o}}picos: conceitos de um sistema de navega{\c{c}}{\~{a}}o a partir da an{\'{a}}lise de uma Unidade de Medidas Inerciais (UMI) comercial; teoria de grades de Bragg a fibra {\'{o}}ptica, incluindo-se a an{\'{a}}lise de altera{\c{c}}{\~{a}}o de suas propriedades quando submetida {\`{a}} tra{\c{c}}{\~{a}}o; detalhamento da topologia do aceler{\^{o}}metro proposto, incluindo-se o dimensionamento da massa inercial e da estrutura mec{\^{a}}nica; projetos da fonte de luz, do sistema interrogador e da eletr{\^{o}}nica de apoio, bem como a integra{\c{c}}{\~{a}}o dos sistemas; ensaios realizados, incluindo ensaio est{\'{a}}tico sem excita{\c{c}}{\~{a}}o, ensaio de oscila{\c{c}}{\~{a}}o senoidal, ensaio com velocidade angular constante, ensaio com varia{\c{c}}{\~{o}}es da posi{\c{c}}{\~{a}}o angular intercaladas por repouso, e ensaio de inclina{\c{c}}{\~{a}}o; an{\'{a}}lise dos resultados e propostas de trabalhos futuros. O desenvolvimento do aceler{\^{o}}metro angular aqui proposto {\'{e}} fundamental para que se possa obter, em um futuro pr{\'{o}}ximo, aceler{\^{o}}metros lineares e angulares de seis graus de liberdade (medi{\c{c}}{\~{a}}o de tr{\^{e}}s acelera{\c{c}}{\~{o}}es lineares ortogonais e de tr{\^{e}}s acelera{\c{c}}{\~{o}}es angulares ortogonais), id{\'{e}}ia essa que se encontra em processo de prote{\c{c}}{\~{a}}o de propriedade intelectual, por meio de dep{\'{o}}sito de pedido nacional de patente sob n{\'{u}}mero BR 10 2012 016704-2. Al{\'{e}}m desse dep{\'{o}}sito de pedido nacional de patente, este trabalho gerou, ainda, 1 (uma) trabalho de Inicia{\c{c}}{\~{a}}o Cient{\'{i}}fica CNPq para a aluna Thabata Ferreira Ganga (UNIFESP), 2 (duas) publica{\c{c}}{\~{o}}es em congressos internacionais, 3 (tr{\^{e}}s) publica{\c{c}}{\~{o}}es em eventos nacionais e 1 (uma) submiss{\~{a}}o de proposta de Cap{\'{i}}tulo de Livro.},
  file     = {:teses/Carvalho2013 - Acelerometro Angular Opto Mecanico Baseado Em Grades De Bragg a Fibra Optica.pdf:PDF},
}

