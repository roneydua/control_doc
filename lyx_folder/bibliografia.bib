  keywords = {aircraft navigation;autonomous aerial vehicles;inertial navigation;Kalman filters;nonlinear filters;temporary navigation;velocity measurements;global navigation satellite system;proprioceptive measurements;Kalman filter;angular rate measurements;inertial measurement unit;altitude measurements;revolution speed;propeller;angular velocities;three-dimensional space;DMAN method;process model;measurement model;unmanned aerial vehicle flight experiments;quadrotor navigation;dimensional dynamic-model-aided navigation;multirotor unmanned aerial vehicle;Vehicle dynamics;Three-dimensional displays;Global navigation satellite system;Propellers;Solid modeling;Unmanned aerial vehicles;Aerial vehicle;multirotor;propeller;navigation;velocity;attitude;dynamic model;EKF;UKF;GNSS-denied},
}

