@InProceedings{MatusVargas2017,
  author    = {Matus-Vargas, A. and Rodriguez-Gomez, G. and Martinez-Carranza, J. and Mu{\~{n}}oz-Silva, A.},
  booktitle = {2017 International Conference on Unmanned Aircraft Systems (ICUAS)},
  title     = {Numerical Optimization Techniques for Nonlinear Quadrotor Control},
  year      = {2017},
  month     = jun,
  pages     = {1309--1315},
  abstract  = {Currently, quadrotors have become very popular and have different applications in entertainment, transportation, rescue and military areas, among others. A key activity in the design of quadrotors is the flight control. A well-known flight controller is the PD, which is sufficient to make the quadcopter to hover in the air. One crucial problem with this type of controllers is the coefficient tuning. Typically, linear models are supposed for the latter, however, in this work we study the use of two optimization techniques for coefficient tuning when the dynamic model is nonlinear. These techniques refer to parametric control tuning based on numerical strategies using Conjugate Gradient (CG) optimization. We also analyze the optimal control law for the same nonlinear model by applying the Pontryagin's Maximum Principle, which yields the conditions to numerically find the optimal flight control to transfer the quadrotor between hovering states. We carried out a comparison of the behavior of the quadcopter dynamics regarding the tuned parametric controls obtained with the two CG strategies and with the achieved optimal control. In addition, we analyzed, from the point of view of the user, which one of these strategies is the easiest to implement.},
  doi       = {10.1109/ICUAS.2017.7991483},
  file      = {:artigos/MatusVargas2017 - Numerical optimization techniques for nonlinear quadrotor control.pdf:PDF},
  keywords  = {aircraft control;conjugate gradient methods;helicopters;maximum principle;nonlinear control systems;optimisation;numerical optimization techniques;nonlinear quadrotor control;parametric control tuning;numerical strategies;conjugate gradient optimization;CG optimization;optimal control law;nonlinear model;Pontryagin maximum principle;optimal flight control;quadcopter dynamics;tuned parametric controls;Mathematical model;Tuning;Numerical models;Optimal control;Cost function;PD control},
}

@InProceedings{Carvalho2019,
  author    = {Carvalho, B. and Rodrigues, L.},
  booktitle = {2019 18\textsuperscript{th} European Control Conference (ECC)},
  title     = {Optimal Longitudinal Trajectory Planning for a Quadrotor Uav Including Linear Drag Effects},
  year      = {2019},
  month     = jun,
  pages     = {2689--2694},
  abstract  = {This work proposes a methodology for determining the optimal trajectories of a quadrotor UAV in the sense of a trade-off between a control energy cost and a time-related cost under the influence of linear drag. The consideration of such a trade-off is motivated by the need to find efficient strategies to deal with battery-powered UAVs having limited battery charge. The problem is formulated as a free terminal time optimal control problem using a trade-off cost index and solutions are derived using the Pontryagin's Minimum Principle (PMP). The results show the sensitivity to drag effects of some important quantities such as trajectory smoothness, control energy, and final time. One of the important contributions of the methodology proposed in this paper is that it provides a Pareto optimal curve that is parameterized by the drag coefficient divided by the mass of the vehicle. Extensive simulation plots, including the Pareto optimal curve, show the results of the proposed method for different drag coefficients.},
  doi       = {10.23919/ECC.2019.8796195},
  file      = {:artigos/Carvalho2019 - Optimal Longitudinal Trajectory Planning for a Quadrotor UAV Including Linear Drag Effects.pdf:PDF},
  keywords  = {autonomous aerial vehicles;drag;helicopters;minimum principle;Pareto optimisation;time optimal control;trajectory control;optimal longitudinal trajectory planning;control energy cost;time-related cost;battery-powered UAVs;battery charge;free terminal time optimal control problem;trade-off cost index;Pareto optimal curve;drag coefficient;quadrotor UAV;Pontryagin's minimum principle;linear drag effects},
}

