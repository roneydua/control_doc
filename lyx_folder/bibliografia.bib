@Article{Zhou2021,
  author    = {Rui Zhou and Fengyi Chen and Shuangshuang Li and Ruohui Wang and Xueguang Qiao},
  journal   = {J. Lightwave Technol.},
  title     = {Three-Dimensional Vector Accelerometer Using a Multicore Fiber Inscribed With Three FBGs},
  year      = {2021},
  month     = may,
  number    = {10},
  pages     = {3244--3250},
  volume    = {39},
  abstract  = {We have designed, fabricated, and characterized a fiber-optic three-dimensional vector accelerometer for vibration measurement. The accelerometer is based on fiber Bragg gratings (FBGs) inscribed with three cores of seven-core fiber by femtosecond laser with a phase mask. Through monitoring the wavelength shifts of two of the seven cores, we obtain the vibration azimuthal angle and the acceleration in the XY plane by angle reconstruction and algorithmic calculation. Similarly, by monitoring the wavelength shift of the core fixed along the Z-axis, we obtain the vibration acceleration in the Z-axis direction. The full 3D vector information about the vibration signal is obtained by vector synthesis. The accelerometer has achieved a working frequency bandwidth from 10 to 220 Hz with the maximum sensitivity of 355 pm/g, and the best azimuthal angle accuracy of 0.269{\textless}inline-formula{\textgreater}               {\textless}tex-math{\textgreater}\$^\{\textblackslash\}circ\${\textless}/tex-math{\textgreater}            {\textless}/inline-formula{\textgreater} in the linear range of 0{\textless}inline-formula{\textgreater}               {\textless}tex-math{\textgreater}\$^\{\textblackslash\}circ\${\textless}/tex-math{\textgreater}            {\textless}/inline-formula{\textgreater}--100{\textless}inline-formula{\textgreater}               {\textless}tex-math{\textgreater}\$^\{\textblackslash\}circ\${\textless}/tex-math{\textgreater}            {\textless}/inline-formula{\textgreater} and 170{\textless}inline-formula{\textgreater}               {\textless}tex-math{\textgreater}\$^\{\textblackslash\}circ\${\textless}/tex-math{\textgreater}            {\textless}/inline-formula{\textgreater}--280{\textless}inline-formula{\textgreater}               {\textless}tex-math{\textgreater}\$^\{\textblackslash\}circ\${\textless}/tex-math{\textgreater}            {\textless}/inline-formula{\textgreater}. Moreover, it is possible to increase the overall bandwidth, sensitivity and accuracy of the azimuth of sensors in the XY plane by adjusting the length of the optical fiber and adjusting mechanical parameters to increase the bandwidth and sensitivity of the sensor in Z-axis. Such improvements will further enhance application to small-scale seismic surveys.},
  doi       = {10.1109/JLT.2021.3058240},
  file      = {:artigos/Zhou2021 - Three Dimensional Vector Accelerometer Using a Multicore Fiber Inscribed with Three FBGs.pdf:PDF},
  keywords  = {Accelerometers; Femtosecond lasers; Fiber optic sensors; Multicore fibers; Optical fibers; Space division multiplexing},
  publisher = {Optica Publishing Group},
}

@Article{Tan2005,
  author    = {Chin-Woo Tan and Sungsu Park},
  journal   = {IEEE Transactions on Instrumentation and Measurement},
  title     = {Design of accelerometer-based inertial navigation systems},
  year      = {2005},
  issn      = {1557-9662},
  month     = dec,
  number    = {6},
  pages     = {2520--2530},
  volume    = {54},
  abstract  = {We examine the feasibility of designing an accelerometer-based (or gyroscope-free) inertial navigation system that uses only accelerometer measurements to compute the linear and angular motions of a rigid body. The accelerometer output equation is derived to relate the linear and angular motions of a rigid body relative to a fixed inertial frame. A sufficient condition is given to determine if a configuration of accelerometers is feasible. If the condition is satisfied, the angular and linear motions can be computed separately using two decoupled equations of an input-output dynamical system: a state equation for angular velocity and an output equation for linear acceleration. This simple computation scheme is derived from the corresponding dynamical system equations for a special cube configuration for which the angular acceleration is expressed as a linear combination of the accelerometer outputs. The effects of accelerometer location and orientation errors are analyzed. Algorithms that identify and compensate these errors are developed.},
  doi       = {10.1109/TIM.2005.858129},
  file      = {:artigos/Tan2005 - Design of Accelerometer Based Inertial Navigation Systems.pdf:PDF},
  groups    = {accelerometers, triaxial_accelerometer},
  keywords  = {Inertial navigation;Accelerometers;Gyroscopes;Equations;Angular velocity;Acceleration;Aircraft navigation;Costs;Road transportation;Sufficient conditions;Accelerometer measurements;configuration of accelerometers;error sensitivity analysis;feasibility;gyroscope-free;input–output dynamical system realization},
  publisher = {Institute of Electrical and Electronics Engineers (IEEE)},
}

@Article{Jiang2013,
  author    = {Jiang, Qi and Yang, Meng},
  journal   = {Measurement Science and Technology},
  title     = {Simulation and experimental study of a three-axis fiber Bragg grating accelerometer based on the pull–push mechanism},
  year      = {2013},
  issn      = {1361-6501},
  month     = oct,
  number    = {11},
  pages     = {115105},
  volume    = {24},
  abstract  = {A three-axis fiber Bragg grating accelerometer, which has uniform sensitivities to three axes, has been developed for seismic application. This paper presents the design, simulation and calibration of the three-axis accelerometer. A series of experiments were performed to measure harmonic vibration and shock vibration. The precise acceleration was measured by a PZT accelerometer which provided high sensitivity. The fully described dynamic sensitivity of three-axis accelerometers represented by a 3-by-3 matrix is given. The results indicate that the accelerometer has a sensitivity of 0.068 V g−1 in a measured full scale of ±2.5 m s−2. The cross-axis sensitivity was measured as −75.5 dB, −75.5 dB and −78.2 dB, respectively.},
  doi       = {10.1088/0957-0233/24/11/115105},
  file      = {:artigos/Jiang2013 - Simulation and Experimental Study of a Three Axis Fiber Bragg Grating Accelerometer Based on the Pull–push Mechanism.pdf:PDF},
  groups    = {triaxial_accelerometer},
  publisher = {IOP Publishing},
}

@Article{LopezHiguera2011,
  author   = {Lopez-Higuera, José Miguel and Rodriguez Cobo, Luis and Quintela Incera, Antonio and Cobo, Adolfo},
  journal  = {Journal of Lightwave Technology},
  title    = {Fiber Optic Sensors in Structural Health Monitoring},
  year     = {2011},
  issn     = {1558-2213},
  month    = feb,
  number   = {4},
  pages    = {587--608},
  volume   = {29},
  abstract = {Structural Health Monitoring (SHM) can be understood as the integration of sensing and intelligence to enable the structure loading and damage-provoking conditions to be recorded, analyzed, localized, and predicted in such a way that nondestructive testing becomes an integral part of them. In addition, SHM systems can include actuation devices to take proper reaction or correction actions. SHM sensing requirements are very well suited for the application of optical fiber sensors (OFS), in particular, to provide integrated, quasi-distributed or fully distributed technologies. In this tutorial, after a brief introduction of the basic SHM concepts, the main fiber optic techniques available for this application are reviewed, emphasizing the four most successful ones. Then, several examples of the use of OFS in real structures are also addressed, including those from the renewable energy, transportation, civil engineering and the oil and gas industry sectors. Finally, the most relevant current technical challenges and the key sector markets are identified. This paper provides a tutorial introduction, a comprehensive background on this subject and also a forecast of the future of OFS for SHM. In addition, some of the challenges to be faced in the near future are addressed.},
  doi      = {10.1109/JLT.2011.2106479},
  file     = {:artigos/LopezHiguera2011 - Fiber Optic Sensors in Structural Health Monitoring.pdf:PDF},
  keywords = {Optical fiber sensors;Optical fibers;Optical variables measurement;Temperature measurement;Optical fiber sensors (OFS);OFS applications;OFS market;optical transducers;structural health monitoring (SHM);SHM and OFS challenges},
}

@Article{Zhang2024,
  author   = {Zhang, Bingbing and Ning, Tigang and Pei, Li and Liu, Shengchun and Teng, Yuntian},
  journal  = {IEEE Sensors Journal},
  title    = {A Triaxial Dual FBG Accelerometer Which Can be Used for Seismic Wave Detection},
  year     = {2024},
  issn     = {1558-1748},
  pages    = {1--1},
  abstract = {In order to pick up the low frequency vibration signal, a triaxial dual FBG accelerometer based on a vibration-sensitive structure of cross diaphragm spring is proposed in this paper, then the modal analysis and parameter optimization are done by simulation analysis software, and the structure parameters of the triaxial dual FBG accelerometer are determined by combining the actual processing, at last, a very low frequency vibration testing system is built to test its vibration characteristics. The test results show that the natural frequency of the FBG accelerometer is 85 Hz, and the operating frequency is 0.1-50 Hz. The sensitivity of the three components are 590 pm/g, 390 pm/g, 395 pm/g respectively, the minimum detectable acceleration of accelerometer is 0.01 g and the maximum detectable acceleration is 3.0 g. This kind of FBG accelerometer with high sensitivity in low frequency band provides important reference value for the design of similar accelerometers.},
  doi      = {10.1109/JSEN.2024.3359322},
  file     = {:artigos/Zhang2024 - A Triaxial Dual FBG Accelerometer Which Can Be Used for Seismic Wave Detection.pdf:PDF},
  groups   = {accelerometers},
  keywords = {Accelerometers;Fiber gratings;Vibrations;Springs;Sensors;Sensitivity;Structural engineering;Low frequency vibration;Diaphragm spring;Fiber Bragg grating(FBG);Triaxial},
}

@InProceedings{Nureev2020,
  author    = {Nureev, I. I. and Chistyakov, V. V. and Sakhabutdinov, A. Zh. and Kuznetsov, A. A. and Lipatnikov, K. A.},
  booktitle = {2020 Wave Electronics and its Application in Information and Telecommunication Systems (WECONF)},
  title     = {Differential Accelerometer on Fiber Bragg Structures},
  year      = {2020},
  month     = jun,
  pages     = {1--4},
  abstract  = {The paper provides estimates of the applicability of addressed and non-addressed fiber Bragg structures - FBS (both classical and with one or two phase shifts) as sensitive elements in the measuring system of the accelerometer. The accelerometer is constructed according to a differential scheme in which an inertial mass compresses one FBS and stretches the other by the same amount, depending on the value and direction of the force applied to the inertial mass and causing its acceleration, transmitted along the axis of the optical fiber with FBS. In turn, the extension and contraction of the Bragg structures leads to a shift in their Bragg frequencies. It is proposed to solve the problem of determining the offset of the Bragg wavelength by transferring acceleration information from the optical to the radio frequency range, based on the principles of microwave photonics with its advantages as high signal-to- noise ratio, resolution, accuracy and measurement speed.},
  doi       = {10.1109/WECONF48837.2020.9131436},
  file      = {:artigos/Nureev2020 - Differential Accelerometer on Fiber Bragg Structures.pdf:PDF},
  keywords  = {Microwave measurement;Accelerometers;Optical filters;Optical fiber sensors;Area measurement;Optical fiber couplers;Optical variables measurement;microwave photonics;fiber Bragg grating;addressable fiber Bragg structure;Inertial navigation systems;accelerometer;sensor},
}

