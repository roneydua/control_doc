@Article{Shi2018,
  author   = {Shi, D. and Wu, Z. and Chou, W.},
  journal  = {IEEE Access},
  title    = {Generalized Extended State Observer Based High Precision Attitude Control Of Quadrotor Vehicles Subject To Wind Disturbance},
  year     = {2018},
  issn     = {2169-3536},
  pages    = {32349--32359},
  volume   = {6},
  abstract = {Wind disturbance may significantly degrade the attitude control performance during the flight of quadrotor vehicles. In order to meet the requirement of high-precision attitude control, a generalized extend state observer (GESO)-based disturbance and uncertainty estimation and attenuation control strategy is proposed in this paper. First, the disturbances are considered as a time polynomial function, and the nth order model of disturbances are augmented into the quadrotor dynamics. According to the reconstructed model, a GESO is designed and its stability is proved by the Lyapunov theory. Then, we analyze the influences of the observer order and bandwidth on the estimation accuracy in detail and give the gain tuning guidelines for GESO. Second, we design an attitude tracking controller based on the backstepping method and discuss the stability of the entire system. Finally, the numerical simulations and real-time experiments are carried out to evaluate the performance of the proposed controller with the GESO of different orders. Our results show that the proposed method can achieve precise attitude tracking for a quadrotor subject to wind disturbance. Furthermore, when the bandwidth of the GESO is fixed, the performance of the proposed controller improves with the increases of the GESO order.},
  doi      = {10.1109/ACCESS.2018.2842198},
  file     = {:artigos/Shi2018 - Generalized Extended State Observer Based High Precision Attitude Control of Quadrotor Vehicles Subject to Wind Disturbance.pdf:PDF},
  keywords = {aircraft control;attitude control;control nonlinearities;control system synthesis;helicopters;Lyapunov methods;nonlinear control systems;numerical analysis;observers;polynomials;stability;vehicle dynamics;generalized extended state observer;high precision attitude control;wind disturbance;attitude control performance;generalized extend state observer-based disturbance;uncertainty estimation;attenuation control strategy;time polynomial function;nth order model;quadrotor dynamics;observer order;GESO order;quadrotor vehicles;GESO-based disturbance;Lyapunov theory;numerical simulations;precise attitude tracking;backstepping method;gain tuning guidelines;attitude tracking controller design;Observers;Quaternions;Attitude control;Rotors;Mathematical model;Uncertainty;Quadrotor;high precision;GESO;wind disturbance;attitude control},
}

@InProceedings{Choutri2018,
  author    = {Choutri, K. and Lagha, M. and Dala, L. and Lipatov, M.},
  booktitle = {2018 22\textsuperscript{nd} International Conference on System Theory, Control and Computing (ICSTCC)},
  title     = {Quadrotors Uavs Swarming Control under Leader-followers Formation},
