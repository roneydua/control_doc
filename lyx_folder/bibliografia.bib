  groups    = {quaternio, quadrotor, modelagem, mestrado},
  keywords  = {angular velocity control;autonomous aerial vehicles;blades;differential equations;failure analysis;helicopters;linear systems;Newton method;rotors;shear modulus;stability;trajectory control;vehicle dynamics;mathematical modeling;hexacopter control;unmanned aerial vehicle;UAV;stabilization;trajectory control;rotor;counter-rotating fixedpitch blade;angular velocity control;electric motor;rigid body;differential equation;microcopter dynamics;Newton-Euler equation;Euler-Lagrange equation;Euler-angle parametrization;three-dimensional rotation;coordinate space;failure;dynamical model;linearity;quaternion formulation;stability;Quaternions;Mathematical model;Equations;Rotors;Angular velocity;PD control;Aircraft},
}

@Article{Wu2019b,
