@InBook{Takahashi2025,
  author    = {Hidemi Takahashi and Tomonari Hirotani and Hideyuki Taguchi},
  title     = {Flush Air-Data Sensing System for Hypersonic Flight Experimental Vehicle With Ogive-Shaped Nose},
  year      = {2025},
  abstract  = {This paper describes the design progress on a flush air-data sensing (FADS) system for a hypersonic winged-vehicle with an ogive-shaped nose. Target conditions for the design are Mach 3.0 to 7.0 with angles of attack within 20 and sideslip within 5. Databases of surface pressures on the vehicle nose were generated by numerical simulation. The FADS system was designed to estimate air data in real-time at hypersonic speeds by using some unique combinations of differential pressures obtained at the nose surface which were related to air-data states. The surface pressure database and designed estimation algorithms were validated by comparing with multiple simulation methods and wind tunnel experimental results. Results indicated that the designed FADS system can estimate air data within the uncertainty of 7.0\% for the freestream dynamic pressures and 1.1 for angle of attack in the range of Mach 3.5 to 6.5. Other air data such as freestream Mach number and static pressure as well as the altitude were also successfully estimated along with the premised flight trajectory. Thus, the proposed algorithm can estimate the air data with acceptable level of uncertainty while retaining robustness of estimation to sensor failures with low computational cost.　},
  booktitle = {AIAA SCITECH 2025 Forum},
  doi       = {10.2514/6.2025-1340},
  eprint    = {https://arc.aiaa.org/doi/pdf/10.2514/6.2025-1340},
  url       = {https://arc.aiaa.org/doi/abs/10.2514/6.2025-1340},
}

