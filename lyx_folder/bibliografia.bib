  keywords  = {attitude control;autonomous aerial vehicles;cascade control;compensation;helicopters;manipulators;observers;trajectory control;variable structure systems;acceleration-based disturbance observation evaluation;multicopter control;UAV;autonomous unmanned aerial vehicles;self-induced vortices;aerial manipulation;external forces;small helicopters;DO;onboard IMU;cascaded attitude controller;position tracking controller;quadrotor;orientation angles;singularity-free quaternion;angular velocity signals;boundary-layer integral sliding mode control;PID controllers;backstepping controllers;trajectory tracking;turbulent conditions;compensation;horizontal plane;Attitude control;Quaternions;Angular velocity;Force;Acceleration;Vectors;Uncertainty},
}

@Article{Akhtar2020,
  author   = {Akhtar, A. and Saleem, S. and Waslander, S. L.},
  journal  = {IEEE Access},
  title    = {Path Following for a Class of Underactuated Systems Using Global Parameterization},
  year     = {2020},
  issn     = {2169-3536},
  pages    = {34737--34749},
  volume   = {8},
  abstract = {A large number of both aerial and underwater mobile robots fall in the category of underactuated systems that are defined on a manifold, which is not isomorphic to Euclidean space. Traditional approaches to designing controllers for such systems include geometric approaches and local coordinate-based representations. In this paper, we propose a global parameterization of the special orthogonal group, denoted by SO(3), to design path-following controllers for underactuated systems. In particular, we present a nine-dimensional representation of SO(3) that leads to controllers achieving path-invariance for a large class of both closed and non-closed embedded curves. On the one hand, this over-parameterization leads to a simple set of differential equations and provides a global non-ambiguous representation of systems as compared to other local or minimal parametric approaches. On the other hand, this over-parameterization also leads to uncontrolled internal dynamics, which we prove to be bounded and stable. The proposed controller, when applied to a quadrotor system, is capable of recovering the system from challenging situations such as initial upside-down orientation and also capable of performing multiple flips.},
  doi      = {10.1109/ACCESS.2020.2974153},
  file     = {:artigos/Akhtar2020 - Path Following for a Class of Underactuated Systems Using Global Parameterization.pdf:PDF},
  keywords = {attitude control;autonomous aerial vehicles;control system synthesis;differential equations;mobile robots;nonlinear control systems;path planning;SO(3) groups;path-following controllers design;upside-down orientation;uncontrolled internal dynamics;differential equations;closed embedded curves;nonclosed embedded curves;local coordinate-based representations;Euclidean space;aerial mobile robots fall;path-invariance;nine-dimensional representation;special orthogonal group;geometric approaches;underwater mobile robots fall;global parameterization;underactuated systems;quadrotor system;minimal parametric approaches;nonambiguous representation;Manifolds;Aerospace electronics;Vehicle dynamics;Control design;Tools;Robots;Quaternions;Feedback linearization;nonlinear control;path following;quadrotor;underactuated system},
}

@InProceedings{Fernando2011,
