@InProceedings{Nguyen2017,
  author    = {Nguyen, D. and Saussie, D. and Saydy, L.},
  booktitle = {2017 International Conference on Unmanned Aircraft Systems (ICUAS)},
  title     = {Quaternion-based Robust Fault-tolerant Control of a Quadrotor Uav},
  year      = {2017},
  month     = jun,
  pages     = {1333--1342},
  abstract  = {This paper deals with the active fault-tolerant control (FTC) of a quadrotor in the presence of actuator faults. Generally, it is assumed that the fault has been detected, isolated or identified, and then, one proceeds with a reconfigurable controller. This paper presents a complete active fault-tolerant control system with both fault detection and diagnosis (FDD), and FTC. The proposed FTC design is based on gain-scheduling control in the framework of structured H$\infty$ synthesis. The scheduled gains are parameterized as polynomial functions of the loss of control effectiveness of the quadrotor actuators, estimated by a two-stage Kalman filter. Then, the MATLAB-based function systune tunes the polynomial coefficients to meet the robustness and performance requirement. In comparison with other FTC systems that use switching mechanisms, this smooth self-scheduled controller allows one to avoid undesirable transient phenomena during the controller reconfiguration process. Numerical simulations performed on an underactuated six degrees of freedom quadrotor nonlinear model show the effectiveness of the proposed FTC in accommodating different levels of actuator faults. To avoid singularities associated with Euler angles, unit quaternion representations are used to describe the rotational motion of the quadrotor.},
  doi       = {10.1109/ICUAS.2017.7991516},
  file      = {:artigos/Nguyen2017 - Quaternion Based Robust Fault Tolerant Control of a Quadrotor UAV.pdf:PDF},
  keywords  = {actuators;aerospace control;autonomous aerial vehicles;fault tolerant control;helicopters;Kalman filters;mobile robots;numerical analysis;robust control;telerobotics;quaternion;robust fault tolerant control;quadrotor UAV;active fault-tolerant control;FTC;actuator faults;reconfigurable controller;fault detection and diagnosis;FDD;gain-scheduling control;structured H$\infty$ synthesis;quadrotor actuators;two-stage Kalman filter;MATLAB;switching mechanisms;controller reconfiguration process;numerical simulations;Quaternions;Actuators;Fault tolerance;Fault tolerant systems;Mathematical model;Rotors},
}

@InProceedings{Guerrero2016,
  author         = {Guerrero, M. E. and Abaunza, H. and Castillo, P. and Lozano, R. and Garc{\'{i}}a, C. D.},
  booktitle      = {2016 International Conference on Unmanned Aircraft Systems (ICUAS)},
  title          = {Energy Based Control For A Quadrotor Using Unit Quaternions},
  year           = {2016},
  month          = jun,
  pages          = {144--151},
