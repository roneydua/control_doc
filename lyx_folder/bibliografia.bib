@InProceedings{Loianno2016,
  author    = {Loianno, G. and Watterson, M. and Kumar, V.},
  booktitle = {2016 IEEE International Conference on Robotics and Automation (ICRA)},
  title     = {Visual Inertial Odometry for Quadrotors on Se(3)},
