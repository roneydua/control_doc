  keywords = {control system synthesis;Hinfin control;missile control;nonlinear control systems;Riccati equations;missile longitudinal autopilot design;nonlinear Hinfin control technique;optimal control problems;Hamilton-Jacobi-Bellman equation;state dependent Riccati equation;thetas-D method;Riccati equations;Missiles;Nonlinear equations;Aerodynamics;Regulators;Optimal control;Aerospace engineering;Robust control;Scheduling;Aircraft},
}

