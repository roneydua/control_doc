@InProceedings{KunnappillilMadhusudhanan2012,
  author    = {Kunnappillil Madhusudhanan, A. and Corno, M. and Bonsen, B. and Holweg, E.},
  booktitle = {2012 American Control Conference (ACC)},
  title     = {Solving Algebraic Riccati Equation Real Time for Integrated Vehicle Dynamics Control},
  year      = {2012},
  month     = jun,
  pages     = {3593--3598},
  abstract  = {In this paper we present a comparison study of different computational methods to implement State Dependent Riccati Equation (SDRE) based control in real time for a vehicle dynamics control application. Vehicles are mechatronic systems with nonlinear dynamics. One of the promising nonlinear control methods to control vehicle dynamics is based on SDRE. In this method, an Algebraic Riccati Equation (ARE) is solved at each sample to generate the control signal. However solving ARE is computationally complex. In this work, Extended Kalman Filter (EKF) iterative, Schur, Eigenvector, and Hamiltonian methods to solve ARE real time are implemented and studied for their timing, accuracy, and feasibility. Three methods, Schur, Eigenvector, and Hamiltonian are found to have an average calculation time of 3.9, 2.5, and 1.6 milliseconds on a dSPACE real time processor. This timing is acceptable as the controller sampling time is 10 milliseconds. In addition to the least processing time, the Hamiltonian based approach yields the lowest quadratic cost for SDRE based Integrated Vehicle Dynamics Control (IVDC).},
  doi       = {10.1109/ACC.2012.6314669},
  file      = {:artigos/KunnappillilMadhusudhanan2012 - Solving Algebraic Riccati Equation Real Time for Integrated Vehicle Dynamics Control.pdf:PDF},
  groups    = {sdre,mestrado},
  issn      = {2378-5861},
  keywords  = {Kalman filters;mechatronics;Riccati equations;vehicle dynamics;algebraic Riccati equation;integrated vehicle dynamics control;different computational methods;state dependent Riccati equation;vehicle dynamics control;mechatronic systems;nonlinear dynamics;nonlinear control;control signal;extended Kalman filter;Vehicles;Iterative methods;Real-time systems;Accuracy;Timing;Eigenvalues and eigenfunctions;Riccati equations},
}

