@InProceedings{Bulka2018,
  author    = {Bulka, E. and Nahon, M.},
  booktitle = {2018 IEEE/RSJ International Conference on Intelligent Robots and Systems (IROS)},
  title     = {A Universal Controller For Unmanned Aerial Vehicles},
