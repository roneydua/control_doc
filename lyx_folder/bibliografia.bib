@Article{Ishida2025,
  author   = {Takayuki Ishida and Seisuke Fukuda and Kazuki Kariya and Hiroyuki Kamata and Keiki Takadama and Hirohisa Kojima and Shujiro Sawai and Shinichiro Sakai},
  journal  = {Acta Astronautica},
  title    = {Vision-based navigation and obstacle detection flight results in SLIM lunar landing},
