  keywords = {nonlinear control systems;control system synthesis;Riccati equations;stability;Lyapunov methods;closed loop systems;magnetic levitation;nonlinear controllers;state dependent algebraic Ricatti equations;infinite horizon nonlinear regulation;second order systems;closed loop system equations;global stability analysis;Lyapunov analysis;LaSalle principle;magnetic levitation;control system design;Riccati equations;Nonlinear equations;Control systems;Nonlinear systems;Nonlinear control systems;Sliding mode control;Stability analysis;Error correction;Magnetic analysis;Performance analysis},
}

@InProceedings{Pedroso2013a,
  author    = {Pedroso, M. D. and Nascimento, C. B. and Tusset, A. M. and d. S. Kaster, M.},
