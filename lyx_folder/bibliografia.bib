@Article{Barrau2017,
  author   = {Barrau, A. and Bonnabel, S.},
  journal  = {IEEE Transactions on Automatic Control},
  title    = {The Invariant Extended Kalman Filter As a Stable Observer},
  year     = {2017},
  issn     = {1558-2523},
  month    = apr,
  number   = {4},
  pages    = {1797--1812},
  volume   = {62},
  abstract = {We analyze the convergence aspects of the invariant extended Kalman filter (IEKF), when the latter is used as a deterministic nonlinear observer on Lie groups, for continuous-time systems with discrete observations. One of the main features of invariant observers for left-invariant systems on Lie groups is that the estimation error is autonomous. In this paper we first generalize this result by characterizing the (much broader) class of systems for which this property holds. For those systems, the Lie logarithm of the error turns out to obey a linear differential equation. Then, we leverage this {\textquotedblleft}log-linear{\textquotedblright} property of the error evolution, to prove for those systems the local stability of the IEKF around any trajectory, under the standard conditions of the linear case. One mobile robotics example and one inertial navigation example illustrate the interest of the approach. Simulations evidence the fact that the EKF is capable of diverging in some challenging situations, where the IEKF with identical tuning keeps converging.},
  doi      = {10.1109/TAC.2016.2594085},
  file     = {:artigos/Barrau2017 - The Invariant Extended Kalman Filter As a Stable Observer.pdf:PDF},
  keywords = {continuous time systems;convergence;differential equations;Kalman filters;Lie groups;nonlinear control systems;observers;stability;invariant extended Kalman filter;stable observer;IEKF;deterministic nonlinear observer;continuous-time systems;discrete observations;Lie groups;linear differential equation;log-linear property;mobile robotics;Trajectory;Mathematical model;Observers;Kalman filters;Convergence;Estimation error;Differential equations;Asymptotic observers;Kalman filters;nonlinear filters;observers},
}

