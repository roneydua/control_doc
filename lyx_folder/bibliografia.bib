@Article{Tiwari2013,
  author   = {Tiwari, Umesh and Thyagarajan, Krishna and Shenoy, Mangalpady R. and Jain, Subhash C.},
  journal  = {IEEE Sensors Journal},
  title    = {{EDF-Based Edge-Filter Interrogation Scheme for FBG Sensors}},
  year     = {2013},
  issn     = {1558-1748},
  month    = apr,
  number   = {4},
  pages    = {1315--1319},
  volume   = {13},
  abstract = {A fiber Bragg grating (FBG) sensor employing an interrogation scheme based on erbium-doped fiber (EDF) edge detection filter, with enhanced detection bandwidth and intrinsic temperature insensitivity, is proposed and experimentally demonstrated. The edge filter is based on the spectral dependence of absorption in EDF with the possibility of tailoring the absorption profile using appropriate lengths of EDF. A wide filter bandwidth of about 10 nm and a slope detection sensitivity of 1.0 dB/nm in the C-band is demonstrated and validated by simulation results. The proposed scheme is versatile and allows for simultaneous interrogation of multiple FBGs.},
  doi      = {10.1109/JSEN.2012.2235064},
  file     = {:artigos/Tiwari2013 - EDF Based Edge Filter Interrogation Scheme for FBG Sensors.pdf:PDF},
  keywords = {Optical fiber sensors;Fiber gratings;Optical fibers;Strain;Temperature measurement;Erbium-doped fiber (EDF);fiber Bragg gratings (FBG);fiber optics sensors},
}

@Article{Melle1993,
  author    = {Serge M. Melle and Kexing Liu and Raymond M. Measures},
  journal   = {Appl. Opt.},
  title     = {Practical fiber-optic Bragg grating strain gauge system},
  year      = {1993},
  month     = jul,
  number    = {19},
  pages     = {3601--3609},
  volume    = {32},
  abstract  = {A fiber-optic strain gauge system for use in structural monitoring and smart-structure applications is described. The strain gauge uses a fiber-optic Bragg grating sensor to measure strain and a passive, wavelength demodulation system to determine the wavelength of the narrow-band, backreflected spectrum from the grating sensor. The fiber-optic strain gauge system permits the measurement of both static and dynamic strains with a noise-limited resolution of 0.44microstrain/Hz, a measurement dynamic range of 27.8 dB, and a bandwidth of 250 Hz.},
  doi       = {10.1364/AO.32.003601},
  file      = {:artigos/Melle1993 - Practical Fiber Optic Bragg Grating Strain Gauge System.pdf:PDF},
  keywords  = {Composite materials; Effective refractive index; Fiber Bragg grating sensors; Fiber Bragg gratings; Fiber optic sensors; Semiconductor lasers},
  publisher = {Optica Publishing Group},
  url       = {https://opg.optica.org/ao/abstract.cfm?URI=ao-32-19-3601},
}

